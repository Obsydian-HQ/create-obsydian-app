{
  "name": "create-obsydian-app",
  "version": "0.2.0",
  "description": "CLI tool to scaffold new Obsydian applications. Create cross-platform native apps with one command.",
  "type": "module",
  "bin": {
    "create-obsydian-app": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
