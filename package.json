  "keywords": [
    "obsydian",
    "obsidian",
    "cross-platform",
    "native",
    "scaffold",
    "cli",
    "macos",
    "ios",
    "android",
    "windows",
    "linux",
    "cpp"
  ],
  "author": "Obsydian-HQ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Obsydian-HQ/create-obsydian-app.git"
  },
  "homepage": "https://github.com/Obsydian-HQ/create-obsydian-app#readme",
  "bugs": {
    "url": "https://github.com/Obsydian-HQ/create-obsydian-app/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@bacons/xcode": "^1.0.0-alpha.28",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
